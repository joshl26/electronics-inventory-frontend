  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@reduxjs/toolkit": "^1.9.2",
    "ajv": "^6.12.6",
    "ajv-keywords": "^3.5.2",
    "axios": "^1.3.5",
    "bootstrap": "^5.3.8",
    "classnames": "^2.5.1",
    "d3-scale-chromatic": "^3.0.0",
    "eslint-config-react-app": "^7.0.1",
    "html-webpack-plugin": "^5.6.5",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "mini-css-extract-plugin": "^2.9.4",
    "nanoid": "^5.1.6",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.3.4",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.10.1",
    "react-redux": "^8.0.5",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.9.0",
    "react-slideshow-image": "^4.2.1",
    "react-social-icons": "^5.15.0",
    "recharts": "^2.5.0",
    "sass": "^1.94.2",
    "victory-vendor": "^36.6.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-jest": "^30.2.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^2.8.0",
    "redux-mock-store": "^1.5.5",
    "typescript": "^4.9.5",
    "vite": "^7.2.4",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,json,css,scss}",
    "type-check": "tsc --noEmit",
    "dev": "concurrently \"npm start\" \"npm run type-check -- --watch\"",
    "barrels:check": "node scripts/check-barrels.js",
    "barrels:generate": "node scripts/generate-barrels.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jsconfig": {
    "compilerOptions": {
      "baseUrl": "src",
      "paths": {
        "features/*": [
          "features/*"
        ],
        "auth/*": [
          "auth/*"
        ],
        "shared/*": [
          "shared/*"
        ],
        "app/*": [
          "app/*"
        ],
        "utils/*": [
          "utils/*"
        ],
        "hooks/*": [
          "hooks/*"
        ],
        "img/*": [
          "img/*"
        ],
        "svg/*": [
          "svg/*"
        ]
      }
    },
    "exclude": [
      "node_modules",
      "build"
    ]
  }
}
